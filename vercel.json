{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/url", "dest": "/index.js" },
    { "src": "/static/(.*)", "dest": "/index.js" },
    { "src": "/:shortId", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "server": {
    "maxPayloadSize": "25mb",
    "healthCheck": "/healthz",
    "hostHeader": "x-vercel-id"
  }
}
