{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/url", "dest": "/index.js" },
    { "src": "/static/(.*)", "dest": "/index.js" },
    { "src": "/:shortId", "dest": "/index.js" },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
